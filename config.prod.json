{
  "inspector": {
    "region": "west-coast"
  },
  "metrics_db": [
    {
      "influxdb": {
        "database_url": "inspector-influxdb",
        "port": 8086,
        "database_name": "inspector",
        "transport_protocol": ""
      }
    }
  ],
  "targets": [
    {
      "id": "gh",
      "name": "Github's pricing page",
      "probers": [
        {
          "id": "GH",
          "name": "basic_http_prober",
          "context": {
            "url": "https://github.com/pricing",
            "method": "GET",
            "parameters": {
              "param1": "foo",
              "param2": "bar"
            },
            "cookies": {
              "name": "value"
            },
            "allow_redirects": false,
            "timeout": "6s"
          }
        }
      ]
    },
    {
      "id": "sumdnotes",
      "name": "Sumnotes",
      "probers": [
        {
          "id": "Sumnotes",
          "name": "basic_http_prober",
          "context": {
            "url": "https://sumnotes.net",
            "method": "GET",
            "parameters": {},
            "cookies": {},
            "allow_redirects": false,
            "timeout": "12s"
          }
        }
      ]
    },
    {
      "id": "google",
      "name": "Google's home page",
      "probers": [
        {
          "id": "Google",
          "name": "basic_http_prober",
          "context": {
            "url": "https://www.google.com",
            "method": "GET",
            "parameters": {},
            "cookies": {},
            "allow_redirects": true,
            "timeout": "5s"
          }
        }
      ]
    }
  ]
}
